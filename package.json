{
  "name": "rag-whatsapp-assistant",
  "version": "0.1.0",
  "private": true,
  "description": "RAG-powered WhatsApp assistant with Vercel deploy",
  "workspaces": [
    "frontend",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace frontend",
    "build": "npm run build --workspace @rag-whatsapp-assistant/shared && npm run build --workspace frontend",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "format": "npm run format --workspaces --if-present",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel --prod"
  },
  "keywords": [
    "whatsapp",
    "rag",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@10.9.0",
  "dependencies": {
    "@rag-whatsapp-assistant/shared": "workspace:*",
    "firebase": "^11.0.2",
    "firebase-admin": "^12.7.0",
    "busboy": "^1.6.0",
    "node-fetch": "^3.3.2",
    "react-router-dom": "^7.9.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.8.4",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vercel/node": "^3.2.29",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  }
}
